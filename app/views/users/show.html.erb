<% provide(:title, "Your profile") %>
<h1>Current profile</h1>

<div class="signup">
  <div class="container">
    
    <!-- Your form here -->
    <%= form_for(@user, url: user_path(current_user)) do |f| %>
      <%= f.text_field :first_name %>
      <%= f.text_field :last_name %>
      <%= f.email_field :email %>
    <% end %>

<p><%= link_to(root_path) %></p>

  </div>
</div>